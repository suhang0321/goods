<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.suhang.library.mapper.AdminMapper">
    <select id="findAdminByNameAndPwd" parameterType="com.suhang.library.po.Admin" resultType="com.suhang.library.po.Admin">
        SELECT * FROM t_admin WHERE adminname = #{adminname} AND adminpwd = #{adminpwd}
    </select>
</mapper>
